{
  "name": "desktop",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:web": "vite --host 127.0.0.1 --port 5175",
    "build": "tsc -b && vite build",
    "lint": "eslint . --ext .ts,.tsx",
    "preview": "vite preview --host",
    "tauri": "tauri",
    "test": "playwright test",
    "test:e2e": "pnpm exec kill-port 5175 && cross-env START_SERVER_AND_TEST_KILL_COMMAND=\"taskkill /F /T /PID\" start-server-and-test dev:web http://127.0.0.1:5175 \"playwright test\" && pnpm exec kill-port 5175",
    "test:unit": "vitest run"
  },
  "dependencies": {
    "@tauri-apps/api": "^2.8.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@playwright/test": "^1.55.0",
    "@tauri-apps/cli": "^2.8.4",
    "@typescript-eslint/eslint-plugin": "^8.44.0",
    "@typescript-eslint/parser": "^8.44.0",
    "@types/node": "^24.5.2",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-unused-imports": "^4.2.0",
    "kill-port": "^2.0.1",
    "prettier": "^3.6.2",
    "start-server-and-test": "^2.1.1",
    "typescript": "~5.8.3",
    "vite": "^7.1.6",
    "vitest": "^2.1.8"
  }
}
